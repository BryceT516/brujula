#%RAML 1.0
title: Dropbox API
version: 1
baseUri: https://api.dropbox.com/{version}
securedBy: [oauth_2_0]
securitySchemes:
  - oauth_2_0: !include securitySchemes_Dropbox/oauth_2_0.raml
  - oauth_1_0: !include securitySchemes_Dropbox/oauth_1_0.raml
/users:
  get:
    securedBy: [oauth_2_0, oauth_1_0]
